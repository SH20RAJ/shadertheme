{
  "name": "ShaderTheme Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#1A1A1A",
    "editor.foreground": "#E0E0E0",
    "activityBar.background": "#151515",
    "activityBar.foreground": "#BDBDBD",
    "sideBar.background": "#171717",
    "statusBar.background": "#101010",
    "statusBar.foreground": "#9E9E9E",
    "titleBar.activeBackground": "#151515",
    "titleBar.activeForeground": "#BDBDBD",
    "tab.activeBackground": "#202020",
    "tab.inactiveBackground": "#1A1A1A",
    "editorGroupHeader.tabsBackground": "#171717",
    "input.background": "#202020",
    "dropdown.background": "#202020",
    "panel.background": "#171717",
    "menu.background": "#171717",
    "list.activeSelectionBackground": "#303030",
    "list.hoverBackground": "#252525",
    "editor.lineHighlightBackground": "#25252550",
    "editor.selectionBackground": "#404040",
    "editor.wordHighlightBackground": "#35353580",
    "editor.findMatchBackground": "#454545",
    "editor.findMatchHighlightBackground": "#45454580",
    "editorBracketMatch.background": "#404040",
    "editorCursor.foreground": "#BDBDBD",
    "editorWidget.background": "#202020",
    "peekView.border": "#404040",
    "peekViewEditor.background": "#1A1A1A",
    "peekViewResult.background": "#202020",
    "peekViewTitle.background": "#171717",
    "scrollbarSlider.background": "#40404080",
    "scrollbarSlider.hoverBackground": "#505050A0",
    "scrollbarSlider.activeBackground": "#606060A0"
  },
  "tokenColors": [
    {
      "name": "Comments",
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#707070",
        "fontStyle": "italic"
      }
    },
    {
      "name": "String",
      "scope": ["string", "string.template"],
      "settings": {
        "foreground": "#B0B0B0"
      }
    },
    {
      "name": "Numbers and Constants",
      "scope": ["constant.numeric", "constant.language", "constant.character", "constant.other"],
      "settings": {
        "foreground": "#9E9E9E"
      }
    },
    {
      "name": "Keywords",
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#D4D4D4",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Functions",
      "scope": ["entity.name.function", "support.function"],
      "settings": {
        "foreground": "#C0C0C0",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Classes and Types",
      "scope": ["entity.name.type", "entity.name.class", "entity.name.namespace", "entity.name.scope-resolution"],
      "settings": {
        "foreground": "#E0E0E0",
        "fontStyle": "bold"
      }
    },
    {
      "name": "Variables",
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#A8A8A8"
      }
    },
    {
      "name": "Parameters",
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#959595",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Properties",
      "scope": ["variable.other.property", "variable.other.object.property"],
      "settings": {
        "foreground": "#ABABAB"
      }
    },
    {
      "name": "Operators",
      "scope": ["keyword.operator"],
      "settings": {
        "foreground": "#CCCCCC"
      }
    },
    {
      "name": "Decorators",
      "scope": ["meta.decorator", "meta.decorator variable.other", "meta.decorator punctuation.decorator"],
      "settings": {
        "foreground": "#8A8A8A",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Tags",
      "scope": ["entity.name.tag", "punctuation.definition.tag"],
      "settings": {
        "foreground": "#BEBEBE"
      }
    },
    {
      "name": "Attributes",
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#A0A0A0"
      }
    }
  ],
  "semanticHighlighting": true,
  "semanticTokenColors": {
    "enumMember": "#B8B8B8",
    "property": "#A8A8A8",
    "parameter": "#959595",
    "function": "#C0C0C0",
    "method": "#C0C0C0",
    "class": "#E0E0E0",
    "type": "#E0E0E0",
    "interface": "#D4D4D4"
  }
}
