{
  "name": "shadertheme",
  "displayName": "ShaderTheme",
  "description": "A highly customizable VS Code theme with focus on readability and aesthetics",
  "version": "1.0.0",
  "publisher": "shadertheme",
  "engines": {
    "vscode": "^1.60.0"
  },
  "categories": [
    "Themes"
  ],
  "keywords": [
    "theme",
    "dark theme",
    "light theme",
    "customizable",
    "shader"
  ],
  "contributes": {
    "themes": [
      {
        "label": "ShaderTheme Dark",
        "uiTheme": "vs-dark",
        "path": "./themes/shadertheme-dark.json"
      },
      {
        "label": "ShaderTheme Light",
        "uiTheme": "vs",
        "path": "./themes/shadertheme-light.json"
      }
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/username/shadertheme"
  },
  "scripts": {
    "package": "vsce package",
    "publish": "vsce publish"
  },
  "devDependencies": {
    "vsce": "^2.15.0"
  }
}
